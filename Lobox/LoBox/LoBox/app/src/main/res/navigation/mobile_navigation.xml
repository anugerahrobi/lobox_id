<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.lordania.lobox.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_coin"
        android:name="com.lordania.lobox.ui.coin.Coin"
        android:label="@string/menu_coin"
        tools:layout="@layout/fragment_coin" />

    <fragment
        android:id="@+id/nav_help"
        android:name="com.lordania.lobox.ui.help.Help"
        android:label="@string/menu_help"
        tools:layout="@layout/fragment_help" />

    <fragment
        android:id="@+id/nav_profil"
        android:name="com.lordania.lobox.ui.profil.Profil"
        android:label="@string/menu_profil"
        tools:layout="@layout/fragment_profil"/>
    <fragment
        android:id="@+id/nav_logout"
        android:name="com.lordania.lobox.ui.login.login"
        android:label="@string/menu_login"
        tools:layout="@layout/fragment_login"/>
         <action
            android:id="@+id/action_logout"
            app:destination="@id/nav_logout"/>
</navigation>